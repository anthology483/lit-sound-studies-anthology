      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Can patter out their hasty orisons."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
