      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And bugles calling for them from sad shires."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
