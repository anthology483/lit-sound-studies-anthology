      "start_time": 25,
      "end_time": 27,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Nor any voice of mourning save the choirs,—"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        }
      ],
