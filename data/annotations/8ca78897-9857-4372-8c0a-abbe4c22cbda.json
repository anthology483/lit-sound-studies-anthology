      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Orison - A prayer"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Definition",
          "category": "Background"
        }
      ],
      "uuid": "36b189af-6e08-4f8e-9bce-64771c25ee19"
    },
    {
      "start_time": 21,
      "end_time": 24,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "No mockeries now for them; no prayers nor bells;"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Amplitude",
