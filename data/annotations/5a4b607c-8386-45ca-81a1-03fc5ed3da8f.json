      "start_time": 118,
      "end_time": 119,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "that i dance"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Caesura",
          "category": "Form"
        },
        {
          "tag": "Ambient Noise",
          "category": "Extra-Poetic Sounds"
        }
      ],
      "uuid": "746650d0-a018-42e3-9abd-a11b2c4ff028"
    },
    {
      "start_time": 121,
      "end_time": 125,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "as if i have diamonds at the meetings of my thighs"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        }
      ],
      "uuid": "230ab428-4d81-4d3c-b3d3-4603b4f07865"
    },
    {
      "start_time": 126,
      "end_time": 129,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Out of the huts of history shame i rise"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
