    },
    {
      "start_time": 142,
      "end_time": 144,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Leaving behind nights of terror and fear"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
      ],
      "uuid": "0e40718d-74fa-4bac-8848-8730dc91b735"
    },
    {
      "start_time": 145,
