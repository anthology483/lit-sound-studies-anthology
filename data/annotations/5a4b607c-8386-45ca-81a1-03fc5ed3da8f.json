    {
      "start_time": 137,
      "end_time": 141,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Welling and swelling I bear in the tide."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
