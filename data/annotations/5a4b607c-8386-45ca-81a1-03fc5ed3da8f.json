      "uuid": "04444277-191f-4c60-9374-e6b1af0469d3"
    },
    {
      "start_time": 130,
      "end_time": 132,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "up from a past rooted in pain i rise"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
      ],
      "uuid": "a773b330-5090-4b79-bb2d-7f17422b0339"
    },
