      "end_time": 136,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I'm a black ocean, leaping and wide,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        }
